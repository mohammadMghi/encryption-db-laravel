{
  "name": "whitestarcode/encryptiondb",
  "description": "A Laravel DB encryption package",
  "type": "library",
  "autoload": {
    "psr-4": {
      "WhiteStarCode\\DbCipher\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
   "extra": {
        "laravel": {
            "providers": [
                "WhiteStarCode\\DbCipher\\DbCipherServiceProvider"
            ],
            "aliases": {
                "Encryption": "WhiteStarCode\\DbCipher\\Facades\\Encryption"
            }
        }
    }
}